{
    "name": "fireball",
    "license": "MIT",
    "version": "0.1.0",
    "private": true,
    "url": "https://github.com/orden-gg/fireball",
    "type": "module",
    "dependencies": {
        "@apollo/client": "^3.3.12",
        "@emotion/css": "^11.5.0",
        "@emotion/react": "^11.5.0",
        "@emotion/styled": "^11.3.0",
        "@graphprotocol/graph-cli": "0.35.0",
        "@graphprotocol/graph-ts": "0.28.1",
        "@ion-phaser/react": "^1.3.0",
        "@mui/icons-material": "^5.10.16",
        "@mui/lab": "^5.0.0-alpha.110",
        "@mui/material": "^5.10.16",
        "@mui/styles": "^5.10.16",
        "@reduxjs/toolkit": "1.8.3",
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^11.2.5",
        "@testing-library/user-event": "^12.8.1",
        "axios": "^0.21.1",
        "axios-cache-adapter": "^2.7.3",
        "classnames": "^2.2.6",
        "cross-fetch": "^3.1.4",
        "ethers": "^5.4.6",
        "graphql": "^15.5.0",
        "lodash": "^4.17.21",
        "luxon": "^2.3.1",
        "phaser": "^3.55.2",
        "query-string": "^7.1.1",
        "quickswap-sdk": "^3.0.8",
        "react": "17.0.2",
        "react-blockies": "^1.4.1",
        "react-content-loader": "^6.0.3",
        "react-data-components": "^1.2.0",
        "react-dom": "17.0.2",
        "react-helmet": "^6.1.0",
        "react-id-generator": "^3.0.2",
        "react-redux": "8.0.2",
        "react-responsive-carousel": "^3.2.21",
        "react-router-dom": "6.3.0",
        "react-scripts": "4.0.3",
        "react-swipeable-views": "^0.14.0",
        "react-virtuoso": "^2.16.1",
        "serve": "^12.0.1",
        "use-metamask": "^1.3.2",
        "web-vitals": "^1.1.0"
    },
    "scripts": {
        "build": "yarn lint && NODE_OPTIONS=--openssl-legacy-provider GENERATE_SOURCEMAP=false react-scripts build",
        "build:prod": "gcloud builds submit --tag eu.gcr.io/ordengg/fireball --timeout=1800 --machine-type=N1_HIGHCPU_8",
        "gcloud:staging": "gcloud run deploy fireball-staging --image eu.gcr.io/ordengg/fireball:latest --region europe-west1",
        "gcloud:prod": "gcloud run deploy fireball --image eu.gcr.io/ordengg/fireball:latest --region europe-west1",
        "deploy:staging": "run-s build:prod gcloud:staging",
        "deploy:prod": "run-s build:prod gcloud:prod",
        "start": "DISABLE_ESLINT_PLUGIN=true NODE_OPTIONS=--openssl-legacy-provider PORT=3005 react-scripts start && yarn lint",
        "start:prod": "NODE_OPTIONS=--openssl-legacy-provider react-scripts build && serve -s build",
        "subgraph:codegen": "graph codegen subgraph/subgraph.yaml -o subgraph/generated",
        "subgraph:auth:stage": "dotenv -- cross-var graph auth --product hosted-service %SUBGRAPH_STAGE_API_KEY%",
        "subgraph:auth:prod": "dotenv -- cross-var graph auth --product hosted-service %SUBGRAPH_PROD_API_KEY%",
        "subgraph:deploy:stage": "cd subgraph && graph deploy --node https://api.thegraph.com/deploy/ dudendy/gotchiverse",
        "subgraph:deploy:prod": "cd subgraph && graph deploy --node https://api.thegraph.com/deploy/ orden-gg/gotchiverse",
        "test": "jest --config ./jest.config.js",
        "eject": "react-scripts eject",
        "clean": "rm -rf node_modules",
        "lint": "eslint -c .eslintrc.json --ext .ts,.tsx .",
        "prepare": "husky install",
        "data": "execution() { yarn ts-node --esm src/data/scripts/\"$@\".script.ts; }; execution"
    },
    "paths": {
        "*": [
            "types/*.d.ts"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "immer": "9.0.6",
        "glob-parent": "5.1.2"
    },
    "devDependencies": {
        "@babel/preset-env": "7.16.5",
        "@babel/preset-react": "7.16.5",
        "@types/jest": "^28.1.1",
        "@types/lodash": "^4.14.182",
        "@types/luxon": "^2.3.2",
        "@types/node": "^17.0.41",
        "@types/react": "17.0.2",
        "@types/react-dom": "17.0.2",
        "@types/react-router-dom": "5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "cross-var": "^1.1.0",
        "dotenv": "^16.0.1",
        "dotenv-cli": "^6.0.0",
        "husky": "^8.0.1",
        "jest": "26.6.0",
        "jest-canvas-mock": "^2.3.1",
        "matchstick-as": "0.5.0",
        "npm-run-all": "^4.1.5",
        "react-test-renderer": "17.0.2",
        "regenerator-runtime": "0.13.9",
        "ts-node": "^10.8.1",
        "typescript": "4.7.3"
    }
}
